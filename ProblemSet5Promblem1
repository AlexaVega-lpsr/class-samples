import turtle

def drawrhombus(kok):
  k = 0
  kok.color("purple")
  kok.fillcolor("purple")
  kok.begin_fill()
  while k < 2: 
    kok.left(60)
    kok.forward(20)
    kok.left(120)
    kok.forward(20)
    k = k + 1
    
  kok.end_fill()
  
def drawrhombis(kuk):
  o = 0 
  while o < 1:
    kuk.color("light blue")
    kuk.fillcolor("light blue")
    kuk.begin_fill()
    kuk.right(60)
    kuk.forward(20)
    kuk.right(120)
    kuk.forward(20)
    kuk.right(60)
    kuk.forward(20)
    kuk.right(120)
    kuk.forward(20)
    kuk.end_fill()
    kuk.right(60)
    kuk.color("blue")
    kuk.fillcolor("blue")
    kuk.begin_fill()
    kuk.forward(20)
    kuk.left(120)
    kuk.forward(20)
    kuk.left(60)
    kuk.forward(20)
    kuk.end_fill()
    o = o + 1
  kuk.end_fill()
  
  
kak = turtle.Turtle()
kak.speed(10)
kak.right(30)
drawrhombus(kak)
kak.right(150)
kak.penup()
kak.forward(40)
kak.right(180)
kak.forward(5)
kak.pendown()
kak.right(30)
drawrhombus(kak)
kak.right(150)
